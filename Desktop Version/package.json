{
  "name": "api-commander-desktop",
  "version": "1.0.0",
  "description": "Desktop version of API Commander built with NodeGui.",
  "main": "src/index.js",
  "scripts": {
    "start": "qode ./src/index.js",
    "dev": "nodemon -e js,ts,tsx --ignore dist/ --ignore node_modules/ --exec  qode ./src/index.js",
    "build": " rm -rf ./dist && rm -rf ./deploy && yarn gui:init && yarn web:pack && nodegui-packer --pack ./dist",
    "web:pack": "webpack --mode production",
    "gui:init": "nodegui-packer --init apiCommander",
    "dark:build": "npm run before-build && npm run build:pipeline && npm run format && npm run after-build",
    "build:pipeline": "npm run build:types && npm run build:cjs && npm run build:esm",
    "build:cjs": "npx tsc -p ./tsconfig.cjs.json",
    "build:esm": "npx tsc -p ./tsconfig.esm.json",
    "build:types": "npx tsc -p ./tsconfig.types.json -d",
    "format": "npx prettier --write ./**/*.{js,ts}",
    "after-build": "node ./after-build.js",
    "before-build": "node ./before-build.js"
  },
  "keywords": [
    "nodegui",
    "api",
    "client"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dark-engine/core": "^1.5.1",
    "@dark-engine/platform-desktop": "^1.5.1",
    "@nodegui/nodegui": "0.73.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "axios": "^1.7.8",
    "base-64": "^1.0.0",
    "crypto-js": "^4.2.0",
    "form-data": "^4.0.4",
    "node-fetch": "2",
    "nodegui-stylesheet": "^0.4.0",
    "open": "7.2.1",
    "react": "^18.0.0",
    "vite": "^2.9.1"
  },
  "devDependencies": {
    "@nodegui/packer": "^1.5.0",
    "@types/node": "^18.7.14",
    "@types/react": "^17.0.43",
    "@vitejs/plugin-react": "^1.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "file-loader": "^6.2.0",
    "native-addon-loader": "^2.0.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
